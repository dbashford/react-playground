{
  "name": "react-playground",
  "description": "Playing with react/webpack",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbashford/webpack-playground.git"
  },
  "homepage": "https://github.com/dbashford/webpack-playground",
  "keywords": [],
  "main": "index.html",
  "scripts": {
    "lint": "eslint -c .eslintrc src",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.config.js",
    "watch-client2": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client-watch.js && node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.client-watch.js",
    "watch2": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start\""
  },
  "dependencies": {
    "react": "0.13.3"
  },
  "devDependencies": {
    "webpack": "1.12.1",

    "babel-core": "5.8.20",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.20",
    "babel-plugin-typecheck": "1.2.0",

    "eslint": "1.3.1",
    "eslint-loader": "1.0.0",
    "eslint-config-airbnb": "0.0.8",
    "babel-eslint": "4.1.1",
    "eslint-plugin-react": "3.3.2",

    "node-sass": "3.3.2",
    "css-loader": "0.18.0",
    "autoprefixer-loader": "3.1.0",
    "sass-loader": "2.0.1",
    "style-loader": "0.12.3",
    "extract-text-webpack-plugin": "0.8.2"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}