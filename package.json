{
  "name": "react-playground",
  "description": "Playing with react/webpack",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbashford/webpack-playground.git"
  },
  "homepage": "https://github.com/dbashford/webpack-playground",
  "keywords": [],
  "main": "index.html",
  "scripts": {
    "start": "node ./babel.server",
    "lint": "eslint -c .eslintrc src",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack/dev.config.js",
    "watch-client": "node webpack/webpack-devserver.js",
    "watch-both": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start\""
  },
  "dependencies": {
    "babel": "5.8.23",

    "react": "0.14.0-rc1",
    "react-dom": "0.14.0-rc1",
    "react-router": "1.0.0-rc1",

    "react-redux": "2.1.2",
    "redux": "3.0.0",

    "immutable":"3.7.5",
    "redux-immutablejs":"0.0.6"
  },
  "devDependencies": {
    "webpack": "1.12.1",
    "webpack-dev-server": "1.10.1",

    "babel-core": "5.8.20",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.20",
    "babel-plugin-react-transform": "1.0.3",
    "react-transform-webpack-hmr": "0.1.4",

    "redux-devtools": "2.1.2",

    "eslint": "1.3.1",
    "eslint-loader": "1.0.0",
    "eslint-config-airbnb": "0.0.8",
    "babel-eslint": "4.1.1",
    "eslint-plugin-react": "3.3.2",
    "eslint-plugin-babel": "2.1.1",

    "node-sass": "3.3.2",
    "css-loader": "0.18.0",
    "autoprefixer-loader": "3.1.0",
    "sass-loader": "2.0.1",
    "style-loader": "0.12.3",
    "react-css-modules":"3.2.1",

    "concurrently": "0.1.1",
    "piping": "0.3.0",

    "hapi":"10.5.0",
    "inert":"3.1.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}